{
    "plugin": {
        "In_mdm_gspread": {
            "class": "In_GSpread",
            "url": "https://docs.google.com/spreadsheets/d/1Hc0olzpJdsOKrR_4ictPY5QLw0vxU0fO",
            "file": "mdm.xlsx"
        },
        "In_mdm": {
            "class": "In_Price_boda_xlsx",
            "file": "mdm.xlsx",
            "depends": [
                "In_mdm_gspread"
            ],
            "export": {
                "Out_vShopTenant_db": {
                    "sql": {
                        "tenant": "mdm",
                        "currency": "uah",
                        "idt_auto": true,
                        "product0": "model"
                    }
                }
            },
            "cond": "used",
            "sheet": {
                "COMPUTERS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "cpu": "CPU",
                        "case": "Case type",
                        "disk": "HDD",
                        "ram": "RAM",
                        "os": "License",
                        "vga": "GPU",
                        "qty":"Quantity",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                   "attr": {
                        "cpu_type": {"field": "cpu", "delim": "-", "idx": 0},
                        "ram_size": {"field": "ram"},
                        "storage_size": {"field": "disk", "delim": " ", "idx": 0},
                        "storage_type": {"field": "disk", "delim": " ", "idx": 1},
                        "pc_case_type": {"field": "case"},
                        "pc_os": {"field": "os"}
                    },
                    "title": ["model", "case", "cpu", "ram", "disk", "vga", "os"],
                    "model": ["model", "case", "cpu"]
                },
                "LAPTOPS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "screen": "Size",
                        "cpu": "CPU",
                        "disk": "HDD",
                        "ram": "RAM",
                        "os": "License",
                        "matrix": "Matrix",
                        "touch": "Touch",
                        "qty": "Quantity",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                    "attr": {
                        "cpu_type": {"field": "cpu", "delim": "-", "idx": 0},
                        "ram_size": {"field": "ram"},
                        "storage_size": {"field": "disk", "delim": " ", "idx": 0},
                        "storage_type": {"field": "disk", "delim": " ", "idx": 1},
                        "pc_os": {"field": "os"}
                    },
                     "title": ["model", "screen", "cpu", "ram", "disk", "matrix", "touch", "os"],
                    "model": ["model", "cpu"]
                }
            }
        }
    }
}
