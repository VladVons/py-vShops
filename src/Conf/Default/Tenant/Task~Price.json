{
    "dir_data": "./Data/Tenant",
    "comment": "Export price lists",
    "include": [
        "boda.json",
        "pcdata.json",
        "mdm_x.json"
    ],
    "plugins": [
        "Out_vShopTenant_db",
        "-Out_Save_txt"
    ],
    "common": {
        "sleep_loop": 0.01,
        "save_cache": true
    },
    "plugin": {
        "Out_vShopTenant_db": {
            "sql": {
                "parts": 10,
                "lang": "ua"
            },
            "auth": "{% file Conf/Default/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_boda",
                "-In_pcdata",
                "In_mdm_x"
            ],
            "img_loader": "{% file Conf/Default/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Default/Task.SrvImg.Api.json %}"
        },
        "Out_Save_txt" : {
            "depends": [
                "In_mdm_t"
            ],
            "dir": "./Data/Pl/Out",
            "db_list": {
                "len": 50
            }
        }
    }
}
