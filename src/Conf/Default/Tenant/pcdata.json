{
    "plugin": {
        "In_pcdata_gspread": {
            "class": "In_GSpread",
            "url": "https://docs.google.com/spreadsheets/d/1L0ZDsTXlEDWHAY9cI22FqhMQ051b82wc",
            "file": "pcdata.xlsx"
        },
        "In_pcdata": {
            "class": "In_Price_pcdata_xlsx",
            "file": "pcdata.xlsx",
            "depends": [
                "In_pcdata_gspread"
            ],
            "export": {
                "Out_vShopTenant_db": {
                    "sql": {
                        "tenant": "pcdata",
                        "currency": "uah",
                        "idt_auto": true,
                        "price_auto": false,
                        "product0": "model"
                    }
                }
            },
            "cond": "used",
            "sheet": {
                "COMPUTERS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "cpu": "CPU",
                        "case": "Case type",
                        "disk": "HDD",
                        "ram": "RAM",
                        "os": "License",
                        "dvd": "Optical drive",
                        "vga": "GPU",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                    "attr": {
                        "cpu_type": [
                            ["get", ["cpu"]],
                            ["split", ["-", 0]]
                        ],
                        "storage_size": [
                            ["get", ["disk"]],
                            ["split", [" ", 0]]
                        ],
                        "storage_type": [
                            ["get", ["disk"]],
                            ["split", [" ", 1]]
                        ],
                        "ram_size": "ram",
                        "pc_case_type": "case",
                        "pc_os": "os"
                    },
                    "title": ["model", "case", "cpu", "ram", "disk", "vga", "os"],
                    "model": ["model", "case", "cpu"]
                },
                "MONITORS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "screen": "Size",
                        "grade": "Screen Grade",
                        "color": "Frame color",
                        "stand": "Stand",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                    "attr": {
                        "screen_size": "screen"
                    },
                    "title": ["model", "screen", "grade", "color"]
                },
                "INDUSTRIAL MONITORS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "screen": "Inches",
                        "grade": "Screen Grade",
                        "color": "Frame color",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                    "attr": {
                        "screen_size": "screen"
                    },
                    "title": ["model", "screen", "grade", "color"]
                },
                "PRINTERS": {
                    "skip": 1,
                    "header": 1,
                    "fields": {
                        "model": "Model",
                        "pages": "Page counter",
                        "price_in": "PriceIn",
                        "price": "Price"
                    },
                    "title": ["model"]
                }
 
            }
        }
    }
}
