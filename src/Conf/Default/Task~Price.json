{
    "dir_data": "./Data/Pl",
    "conf": [
        "./Data/Pl/Conf", 
        "./Data/Pl/In/Price.json"
    ],
    "comment": "Used PC",
    "include": [
        "Task~Price_vendors.json",
        "Task~Price_pcdata.json",
        "Task~Price_mdm.json"
    ],
    "plugins": [
        {"name": "Out_Save_txt", "enable": false},

        {"name": "Out_vShopTenant_db", "enable": true},
        {"name": "Out_vShopTenantModel_db", "enable": false},

        {"name": "Out_vShopSitemap_db", "enable": false},
        {"name": "In_Crawl_ean_txt", "enable": false},
        {"name": "In_Crawl_model_xlsx", "enable": false},
        {"name": "Out_vShopCrawl_db", "enable": false},
        {"name": "Out_Save_xlsx", "enable": false}
    ],
    "common": {
        "sleep_loop": 0.01,
        "save_cache": true
    },
    "plugin": {
        "PricesPl": {
            "depends": [
                "In_Price_pcdata_xlsx"
            ]
        },
        "Out_Save_txt" : {
            "depends": [
                "In_Price_oster1c7_json",
                "-In_Price_mdm_xlsx",
                "-In_Price_comelit_xml",
                "-In_Price_oster1c_xml",
                "-In_Price_pcdata_xlsx"
            ],
            "dir": "./Data/Pl/Out",
            "db_list": { 
                "len": 50
            }
        },
        "Out_Save_xlsx" : {
            "depends": [
                "In_Price_comelit_xml",
                "-In_Price_oster1c_xml",
                "-In_Price_pcdata_xlsx",
                "-In_Crawl_icecat_xlsx"
            ],
            "dir": "./Data/Pl/Out"
        },
        "Out_vShopTenant_db": {
            "sql": {
                "parts": 100,
                "lang_id": 1
            },
            "auth": "{% file Conf/Default/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_Price_oster1c_xml",

                "-In_Price_comelit_xml",
                "-In_Price_pcdata_xlsx",
                "In_Price_mdm_xlsx"
            ],
            "img_loader": "{% file Conf/Default/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Default/Task.SrvImg.Api.json %}"
        },
        "Out_vShopCrawl_db": {
            "sql": {
                "parts": 50,
                "lang_id": 1,
                "max_conn": 1,
                "rand_sleep": [0.1, 1.1],
                "parser": "icecat_biz"
            },
            "auth": "{% file Conf/Default/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "-In_Crawl_ean_txt",
                "-In_Crawl_kalina_xls",
                "-In_Crawl_fozzy_sqlite",
                "In_Crawl_icecat_xlsx"
            ],
            "img_loader": "{% file Conf/Default/Task.SrvCtrl.Api.json:loader.img %}",
            "img_conf": "{% file Conf/Default/Task.SrvImg.Api.json %}"
        },
        "Out_vShopTenantModel_db": {
            "sql": {
                "lang_id": 1,
                "tenant_id": 3,
                "parts": 50
            },
            "auth": "{% file Conf/Default/Task.SrvModel.Api.json:db_auth %}",
            "depends": [
                "Out_vShopCrawl_db",
                "In_Crawl_icecat_xlsx",
                "-Out_vShopSitemap_db"
            ]
        },
        "Out_vShopSitemap_db": {
            "sql": {
                "url_root": "http://used.1x1.com.ua",
                "lang_id": 1,
                "parts": 1000
            },
            "auth": "{% file Conf/Default/Task.SrvModel.Api.json:db_auth %}",
            "dir": "./Data/Pl/Out/Sitemap"
        }
    }
}
