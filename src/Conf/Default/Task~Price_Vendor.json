{
    "plugin": {
        "In_Crawl_ean_txt": {
            "encoding": "cp1251",
            "delimiter": "|",
            "sheet": {
                "default": {
                    "fields": {
                        "ean": 2,
                        "category": 3,
                        "product": 4
                    }
                }
            }
        },
        "In_Crawl_model_xlsx": {
            "sheet": {
                "default": {
                    "skip": 1,
                    "fields": {
                        "mpn": 2,
                        "url": 3,
                        "image1": 6,
                        "image2": 7,
                        "image3": 8
                    }
                }
            }
        },
        "In_Price_pl01_xlsx": {
            "export": {
                "Out_vShopTenant_db": {
                    "sql": {
                        "tenant_id": 2,
                        "price_id": 3,
                        "auto_idt": true
                    }
                },
                "Out_Save_txt": {
                    "TDbProductEx": {
                        "len": 70,
                        "fields": ["category_id", "mpn", "name", "available", "price"]
                    }
                },
                "Out_Save_xlsx": {
                    "TDbProductEx": {
                        "fields": ["category_id", "mpn", "name", "available", "price"]
                    }
                }
            },
            "url_gspread": "https://docs.google.com/spreadsheets/d/1EIwjTitfj1_oyWS7ralnUCtq8ZH0g3DWBKq3gP4qrvo",
            "sheet": {
                "COMPUTERS": {
                    "skip": 1,
                    "fields": {
                        "model": 2,
                        "cpu": 3,
                        "case": 4,
                        "disk": 6,
                        "ram": 7,
                        "os": 8,
                        "dvd": 9,
                        "vga": 10,
                        "price": 11
                    },
                    "title": ["model", "case", "cpu", "ram", "disk", "os"],
                    "model": ["model", "case"]
                },
                "MONITORS": {
                    "skip": 1,
                    "fields": {
                        "model": 2,
                        "screen": 5,
                        "price": 8,
                        "grade": 3,
                        "color": 7,
                        "stand": 4
                    },
                    "title": ["model", "screen", "grade", "color"]
                }
            }
        },
        "In_Price_oster1c_xml": {
            "encoding": "cp1251",
            "export": {
                "Out_vShopTenant_db": {
                    "sql": {
                        "tenant_id": 3,
                        "price_id": 3
                    }
                }
            }
        },
        "In_Price_prom_xml": {
            "export": {
                "Out_vShopTenant_db": {
                    "sql": {
                        "tenant_id": 1,
                        "price_id": 1
                    }
                }
            }
        }
    }
}
