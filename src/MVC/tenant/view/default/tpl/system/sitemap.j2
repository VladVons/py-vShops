{% extends '_inc/layout1.j2' %}

{% block content %}
<!-- sitemap + -->
<div class="container d-flex justify-content-center align-items-center min-vh-100">
  <div class="card" style="width: 20em;">
    <div class="card-header vBgSlave">
      <h3>{{lang.sitemap}}</h3>
    </div>
    <div class="card-body">
      <form method="post">
        <button type="submit" class="btn btn-primary w-100" name="btn_submit">{{lang.submit}}</button>
      </form>
    </div>
    <div class="card-footer">
      {% if in_process %} {{lang.in_process}} {% endif %}
    </div>
  </div>
</div>
<!-- sitemap - -->
{% endblock %}
