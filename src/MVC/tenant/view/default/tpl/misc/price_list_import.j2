{% extends '_inc/layout1.j2' %}

{% block content %}
<!-- price_list + -->
<div class="container pb-2">
  <div class="d-flex justify-content-center align-items-center min-vh-100">
    <div class="card" style="width: 20em;">
      <div class="card-header vBgSlave">
        <h3>{{lang.import}}</h3>
      </div>
      <div class="card-body">
        <form method="post">
          <button type="submit" class="btn btn-primary w-100" name="btn_submit">{{lang.submit}}</button>
        </form>
      </div>
      <div class="card-footer">
        {% if in_process %} {{lang.in_process}} {% endif %}
      </div>
    </div>
  </div>
</div>
<!-- price_list - -->
{% endblock %}
