<div class="container pb-3">
    <div class="vTitle">{{title}}</div>
    <div class="row">
    {% for Rec in TDbList().Import(data) %}
        <div class="col-lg-2 mb-3 d-flex align-items-stretch">
            <div class="card">
                <img src="{{Rec.thumb}}" class="card-img-top" alt="{{Rec.product_title}}">
                <div class="card-body d-flex flex-column vBgSlave">
                    <h4>{{Rec.price}} грн</h4>
                    <h5 class="card-title">{{Rec.product_title}}</h5>
                    <a href="{{Rec.href}}" class="btn btn-primary mt-auto stretched-link">{{lang.view}}</a>
                </div>
            </div>
        </div>
    {% endfor %}
    </div>
</div>
