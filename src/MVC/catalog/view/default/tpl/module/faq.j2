<!-- faq + -->
<div class="container pb-3">
    <div class="vTitle">{{title}}</div>

    {% for Rec in TDbList().Import(dbl_faq) %}
    <div class="card">
        <div class="card-header" id="heading_{{loop.index}}">
            <button class="border-0" data-bs-toggle="collapse" data-bs-target="#collapse_{{loop.counter}}">
                <h3>{{Rec.title}}</h3>
            </button>
        </div>
        <div id="collapse_{{loop.counter}}" class="collapse" aria-labelledby="heading_{{loop.counter}}">
            <div class="card-body">
                <p>{{Rec.descr}}</p>
            </div>
        </div>
    </div>
    {% endfor %}
</div>
