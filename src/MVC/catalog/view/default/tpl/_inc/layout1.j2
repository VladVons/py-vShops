    <meta name="description" content="multikart">
    <meta name="keywords" content="multikart">
    <meta name="author" content="multikart">
    <link rel="icon" href="/default/assets/images/favicon/22.png" type="image/x-icon">
    <link rel="shortscut icon" href="/default/assets/images/favicon/22.png" type="image/x-icon">
    <title>Multikart - Multi-purpose E-commerce Html Template</title>

    <!-- Icons -->
    <link rel="stylesheet" type="text/css" href="/default/assets/css/vendors/font-awesome.css">

    <!--Slick slider css-->
