    <!-- Bootstrap css -->
    <link rel="stylesheet" type="text/css" href="/default/assets/css/vendors/bootstrap.css">

    <!-- Theme css -->
    <link rel="stylesheet" type="text/css" href="/default/assets/css/style.css">
</head>

<body class="theme-color-27">
    <!-- header+ -->{% include './header.j2' %}<!-- header- -->
    <!-- content_top+ -->{% include './content_top.j2' %}<!-- content_top- -->
    <!-- content_block+ -->{% block content %}{% endblock %}<!-- content_block- -->
    <!-- footer -->{% include './footer.j2' %}<!-- /footer -->

    <!-- tap to top -->
    <div class="tap-top top-cls">
        <div>
            <i class="fa fa-angle-double-up"></i>
        </div>
    </div>
    <!-- tap to top end -->

    <!-- latest jquery-->
    <script src="/default/assets//js/jquery-3.3.1.min.js"></script>

    <!-- slick js-->
    <script src="/default/assets/js/slick.js"></script>
    <script src="/default/assets/js/slick-animation.min.js"></script>

    <!-- menu js-->
    <script src="/default/assets/js/menu.js"></script>
    <script src="/default/assets/js/sticky-menu.js"></script>

    <!-- ajax search js -->
    <script1 src="/default/assets/js/typeahead.bundle.min.js"></script1>
    <script1 src="/default/assets/js/typeahead.jquery.min.js"></script1>
    <script1 src="/default/assets/js/ajax-custom.js"></script1>

